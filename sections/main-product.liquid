        }

        .product {
          flex-direction: row-reverse;
        }

       .shopify-app-block {
          max-width: max-content;
        }

        .product__media-list {
          margin: 0;
          row-gap: 2px;
          padding-bottom: 0 !important;
        }

        .product__media-list li {
          width: 100% !important;
          max-width: 100% !important;
          display: block !important;
        }

        .product__title h1 {
          font-size: 30px;
          letter-spacing: 3px;
          color: #000000;
          font-weight: 300;
          font-family: "Futura BT";
        }

        span.price-item.price-item--regular,
        span.price-item.price-item--sale.price-item--last {
          font-size: 14px;
          letter-spacing: 1px;
          color: #000000;
          font-family: "Futura LT";
        }

        s.price-item.price-item--regular {
          font-size: 12px !important;
          letter-spacing: 1px;
          font-family: "Futura LT";
        }

        span.badge {
          font-family: 'Futura BT';
          text-transform: uppercase;
          font-weight: 700;
          font-size: 11px;
          letter-spacing: 2px !important;
        }

        span.badge.price__badge-sold-out {
          background: #000 !important;
          color: #fff !important;
          position: relative;
          left: 1rem;
        }

        .product-form__input.product-form__input--dropdown {
          display: flex;
          align-items: center;
          max-width: 100%;
          width: 100%;
          border-bottom: 1px solid #bbb;
          margin-bottom: 0;
          padding: 0.5rem 0;
        }

        .product-form__input .select:before, .product-form__input .select:after {
          display: none;
        }

        .product-form__input .select {
          max-width: max-content;
        }

        .product-form__input .select svg {
          right: calc(var(--inputs-border-width) + 0.5rem);
            }

        label.form__label {
          font-size: 14px;
          line-height: 22px;
          color: #000000;
          font-weight: 300;
          font-family: "Futura BT";
          margin: 0;
        }

        .select__select {
          font-size: 14px;
          line-height: 22px;
          color: #000000;
          font-weight: 300;
          font-family: "Futura BT";
        }

        .select__select:focus {
          box-shadow: none;
        }

        variant-selects.no-js-hidden {
          border-top: 1px solid #bbb;
          border-bottom: 1px solid #bbb;
        }

        .size_wrapper {
          border-bottom: none !important;
        }

        .product-form__submit {
          font-size: 16px;
          letter-spacing: 3px;
          color: #ffffff;
          font-family: "Futura LT";
          text-align: center;
          text-transform: uppercase;
          min-height: 4rem;
        }

        .product__description.rte {
          font-size: 16px;
          letter-spacing: 0px;
          line-height: 24px;
          color: #000000;
          font-weight: 300;
          font-family: "Futura BT";
          height: auto;
          margin-bottom: 0;
        }

        .product__description.rte.sh_more {
          height: auto;
        }

        .product__description.rte a {
          margin-right: 1rem;
        }

        .product-thumbnails {
          position: absolute;
          top: 22rem;
          right: 3rem;
          z-index: 99;
          display: none;
        }

        .product-thumbnails ul {
          margin: 0;
          padding: 0;
          list-style: none;
        }

        .product-thumbnails ul li a img {
          border: 1px solid #000;
        }

        .product__info-container {
          position: sticky;
          top: 3rem;
        }

        .product-media-modal__toggle svg path {
          fill: #000;
        }

        .thumbnail-slider {
          display: none;
        }

        .service_btns {
          display: grid;
          grid-template-columns: 1fr 1fr;
          gap: 2rem;
          align-items: center;
